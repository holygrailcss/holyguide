---
layout: base.njk
---


{% set parts = content | split('/n') %}



<div class="container-fluid">
  {% include '_website/hero.njk' %}
</div>

<section class="front-section bg-light">
  <div class="container-full   align-middle">
    <div class="row">
      <div class="col-xs-12 col-sm-8">
        <div class="fluid-6-b c-primary">
          {{ presentacion }}
        </div>
      </div>
    </div>
  </div>
</section>


<section>
  <div class="row">
    <div class="col-xs-12">
      <div class="main-content innermarkdown ">
        
        
        <div class="part1">
          parte 1
          {{ parts[0] | safe }}
        </div>

        






        <h2>Primarios</h2>
        <div class="row">
          <div class="col-xs-12">
            {% for color in global.coloresPrimarios %}
              <div class="guide-colors text-center">
                <div class="guide-colors-bg text-center" style="background-color:{{ color.hex }};"></div>
                <div class="guide-colors-text">
                  <div class="title-m">
                    <span>Hex:</span>
                    {{ color.hex }}
                  </div>
                  <div class="title-m">
                    <span>Variable Color:</span> ${{ color.variable }}
                  </div>
                  <div class="title-m">
                    <span>Font color:</span> .{{ color.font }}</div>
                  <div class="title-m">
                    <span >Background Color:</span>
                    .{{ color.bg }}</div>
                </div>
              </div>
            {% endfor %}
          </div>
        </div>



