<script>
    // Botones para cerrar menú
    document.querySelectorAll('#cerrar-menu').forEach(button => {
        button.addEventListener('click', function () {
            document.body.className = '';
        });
    });
    document.querySelector('.js-abrir-menu-men').addEventListener('click', function () {
        document.body.classList.add('has-menu');
        document.body.classList.remove('has-section-woman', 'has-section-accessories', 'has-section-explore');
        document.body.classList.add('has-section-men');
        const sectionMen = document.querySelector('.section-men');
        const firstButton = sectionMen.querySelector('button');
        firstButton.setAttribute('tabindex', '0');
        firstButton.focus();
    });
    document.querySelector('.js-abrir-menu-men2').addEventListener('click', function () {
        if (document.body.classList.contains('has-section-men')) {
            document.body.className = '';
        } else {
            document.body.classList.add('has-menu');
            document.body.classList.remove('has-section-woman', 'has-section-accessories', 'has-section-explore');
            document.body.classList.add('has-section-men');
            const sectionMen = document.querySelector('.section-men');
            const firstButton = sectionMen.querySelector('button');
            firstButton.setAttribute('tabindex', '0');
            firstButton.focus();
        }
    });
    document.querySelector('.js-abrir-menu-woman').addEventListener('click', function () {
        document.body.classList.add('has-menu');
        document.body.classList.remove('has-section-men', 'has-section-accessories', 'has-section-explore');
        document.body.classList.add('has-section-woman');
    });
    document.querySelector('.js-abrir-menu-accessories').addEventListener('click', function () {
        document.body.classList.add('has-menu');
        document.body.classList.remove('has-section-men', 'has-section-woman', 'has-section-explore');
        document.body.classList.add('has-section-accessories');
    });
    document.querySelector('.js-abrir-menu-explore').addEventListener('click', function () {
        document.body.classList.add('has-menu');
        document.body.classList.remove('has-section-men', 'has-section-woman', 'has-section-accessories');
        document.body.classList.add('has-section-explore');
    });
    window.addEventListener('scroll', function () {
        if (window.scrollY > 50) {
            document.body.classList.add('has-scroll');
        } else {
            document.body.classList.remove('has-scroll');
