---
layout: base-clean.njk
---

<!-- Contenido de la plantilla store-list.njk -->
<div class="container-fluid">
  {% include '_website/hero.njk' %}
</div>
<div class="main-content innermarkdown ">{{ content | safe }}</div>

<div class="main-content">

  <section class="mb-32 mt-32">
    <div class="row">
      <div class="col-xs-12 mb-32">
        <div class="code-view ">
          <div class="check-new"></div>
          {# -------- #}
          <div class="is-flex w-100 bg-line pb-8">
            <div class="col-xs-9">
              <div class="p-m-b">Goya</div>
              <div class="p-xs">Calle de Goya, 85, Madrid</div>
              <div class="p-xs is-info mb-16">+34 91 4323900</div>
              <div class="p-xs is-info"> Massimo Dutti Studio </div>
            </div>
            <div class="col-xs-3 is-flex flex-column content-sp-between">
              <div class=" p-xs text-right">
                <div class="p-xs c-dark-grey">2.1 km</div>
              </div>
              <div class=" p-xs text-right">
                <div class="p-xs c-dark-grey">ICON</div>
              </div>
            </div>
          </div>
          {# -------- #}
        </div>
        {% set standardcode %}
        
        {% endset %}
        <pre><code class="language-markup">{{standardcode}}</code></pre>
      </div>
    </div>
  </section>

</div>