---
layout: base.njk
---

<!-- Contenido de la plantilla store-list.njk -->
<div class="container-fluid">
  {% include '_website/hero.njk' %}
</div>
<div class="main-content innermarkdown ">{{ content | safe }}</div>

<div class="main-content">

  <section class="mb-32 mt-32">
    <div class="row">
      <div class="col-xs-12 mb-32">
        <div class="code-view ">
          <div class="check-new"></div>
          {# -------- #}
          <div class="is-flex w-100 bg-line py-32">
            <div class="col-xs-9">
              <div class="p-m-b mb-8">A CORUÑA</div>
              <div class="p-m">CC MYANMAR, LOC</div>
            </div>
            <div class="col-xs-3 is-flex flex-column content-sp-between">
              <div class=" p-xs text-right">
                <div class="p-m">WOMEN & MEN</div>
              </div>
            </div>
          </div>
          {# -------- #}
        </div>
        {% set standardcode %}
        <div class="is-flex w-100 bg-line py-32">
          <div class="col-xs-9">
            <div class="p-m-b mb-8">A CORUÑA</div>
            <div class="p-m">CC MYANMAR, LOC</div>
          </div>
          <div class="col-xs-3 is-flex flex-column content-sp-between">
            <div class=" p-xs text-right">
              <div class="p-m">WOMEN & MEN</div>
            </div>
          </div>
        </div>
        {% endset %}
        <pre><code class="language-markup">{{standardcode}}</code></pre>
      </div>
    </div>
  </section>

</div>