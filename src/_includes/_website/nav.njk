<script>
  document.addEventListener('DOMContentLoaded', function() {
    const sidebar = document.querySelector('.sidebar');
    const mostrarSidebar = document.getElementById('mostrarSidebar');
    const ocultarSidebar = document.getElementById('ocultarSidebar');

    mostrarSidebar.addEventListener('click', function() {

      document.body.classList.add('abrirmenu');
      document.body.classList.remove('quitarmenu');
    });

    ocultarSidebar.addEventListener('click', function() {

      document.body.classList.add('quitarmenu');
      document.body.classList.remove('abrirmenu');

    });

    // Definir las animaciones
    const estiloAnimacion = document.createElement('style');
    estiloAnimacion.textContent = `
      @keyframes deslizarEntrada {
        from { transform: translateX(-100%); }
        to { transform: translateX(0); }
      }
      @keyframes deslizarSalida {
        from { transform: translateX(0); }
        to { transform: translateX(-100%); }
      }
    `;
    document.head.appendChild(estiloAnimacion);
  });
</script>