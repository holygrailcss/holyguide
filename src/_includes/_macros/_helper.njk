{% macro token(tokenType='', explicacion='',dark='') %}
<h3>{{ tokenType }}</h3>
<p class="title-m">{{ explicacion }}</p>
<div class="resize-mini">
  <div class="resize-mini-content-narrow {{dark  }} ">
    {% include 'components/' + tokenType + '.njk' %}
  </div>
</div>
{% set standardcode %}
{% include 'components/' + tokenType + '.njk' %}
{% endset %}
<pre><code class=" ">{{standardcode }} </code></pre>
{% endmacro %}

{% macro insert(elementType='', elements) %}
<div class="mb-32 ">
  <div class="code-view ">
    <div class="check-new"> </div>
    <div class="check-rtl"> </div>
    {% for element in elements %}
    <{{ elementType }} class="{{ element }}">
      {% if elementType == 'icon' %}
      <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
        <path fill-rule="evenodd" clip-rule="evenodd" d="M3.5 8.24264C3.5 7.9665 3.72386 7.74264 4 7.74264L7.74264 7.74264L7.74264 4C7.74264 3.72386 7.9665 3.5 8.24264 3.5C8.51878 3.5 8.74264 3.72386 8.74264 4L8.74264 7.74264H12.4853C12.7614 7.74264 12.9853 7.9665 12.9853 8.24264C12.9853 8.51878 12.7614 8.74264 12.4853 8.74264H8.74264L8.74264 12.4853C8.74264 12.7614 8.51878 12.9853 8.24264 12.9853C7.9665 12.9853 7.74264 12.7614 7.74264 12.4853L7.74264 8.74264L4 8.74264C3.72386 8.74264 3.5 8.51878 3.5 8.24264Z" fill="#292929"></path>
      </svg>
      {% else %}
      {{ element }}
      {% endif %}
    </{{ elementType }}>
    {% endfor %}
  </div>
  {% set standardcode %}
  {% for element in elements %}<{{ elementType }} class="{{ element }}">
    {% if elementType == 'icon' %}
    <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
      <path fill-rule="evenodd" clip-rule="evenodd" d="M3.5 8.24264C3.5 7.9665 3.72386 7.74264 4 7.74264L7.74264 7.74264L7.74264 4C7.74264 3.72386 7.9665 3.5 8.24264 3.5C8.51878 3.5 8.74264 3.72386 8.74264 4L8.74264 7.74264H12.4853C12.7614 7.74264 12.9853 7.9665 12.9853 8.24264C12.9853 8.51878 12.7614 8.74264 12.4853 8.74264H8.74264L8.74264 12.4853C8.74264 12.7614 8.51878 12.9853 8.24264 12.9853C7.9665 12.9853 7.74264 12.7614 7.74264 12.4853L7.74264 8.74264L4 8.74264C3.72386 8.74264 3.5 8.51878 3.5 8.24264Z" fill="#292929"></path>
    </svg>
    {% else %}
    {{ element }}
    {% endif %}
  </{{ elementType }}>
  {% endfor %} {% endset %}
  <div class="code-zone">
    <pre><code class=" language-markup">{{standardcode }} </code></pre>
  </div>
</div>
{% endmacro %}


